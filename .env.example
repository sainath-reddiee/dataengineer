# .env.example
# Copy this to .env and fill in your values

# AdSense Configuration
VITE_ADSENSE_PUBLISHER_ID=ca-pub-XXXXXXXXXX
VITE_ADS_ENABLED=false

# Google Analytics
VITE_GA_MEASUREMENT_ID=G-MTMNP6EV9C

# =============================================================================
# PSEO Deployment - Cloudflare R2 Configuration
# =============================================================================
# Get these from: Cloudflare Dashboard → R2 → Overview → Manage R2 API Tokens

R2_ENDPOINT=https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your_access_key_id
R2_SECRET_ACCESS_KEY=your_secret_access_key
R2_BUCKET_NAME=dataengineerhub-pseo

# Optional: For Cloudflare API access
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_API_TOKEN=your_api_token